<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class table="complexclass1" name="jpaoletti.jpm.test.ComplexClass1">
        <id name="id" type="long">
            <generator class="native"/>
        </id>
        <property name="description" />
        <property name="active" type="yes_no"/>
        <property name="amount" type="big_decimal" scale="2" precision="10" />
        <property name="bigstring" type="text" />
        <property name="date" column="`date`" type="date" />
        <property name="datetime" column="`datetime`" type="timestamp" />
        <property name="key" column="`key`" type="string" />
        <property name="password" type="string" />
        <property name="size" column="`size`" type="long" />
    </class>
</hibernate-mapping>
