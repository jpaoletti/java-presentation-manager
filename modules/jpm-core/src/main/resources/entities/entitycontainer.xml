<?xml version="1.0" encoding="UTF-8"?>
<entity id="entitycontainer" clazz="jpaoletti.jpm.core.EntityContainer">
    <dataAccess class="jpaoletti.jpm.ui.DataAccessEntityContainer"/>
    <owner>
        <entityId>pmsession</entityId>
        <entityProperty>containers</entityProperty>
        <entityCollectionClass>java.util.Collection</entityCollectionClass>
    </owner>
    <operations>
        <operation id="list" enabled="true" scope="general" display="all">
            <properties>
                <property name="operation-column-width" value="100px" />
            </properties>
        </operation>
        <operation id="show" enabled="true" scope="item" display="list" />
    </operations>
    <field id="id" />
    <field id="listcount" property="list.total" />
    <field id="listpage" property="list.page" />
</entity>