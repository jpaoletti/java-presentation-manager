<%@tag description="This tag builds a list cell" pageEncoding="UTF-8"%><%@ taglib uri="/WEB-INF/tld/pmfn.tld" prefix="pmfn" %><%@attribute name = "item" required="false" type="java.lang.Object" %><%@attribute name = "entity" required="true" type="jpaoletti.jpm.core.Entity" %><%@attribute name = "field" required="true" type="jpaoletti.jpm.core.Field" %> <%@attribute name = "operation"	required="true" type="jpaoletti.jpm.core.Operation" %><%@attribute name = "field_value" required="false" type="java.lang.Object" %><%@attribute name ="es" required="true" type="jpaoletti.jpm.struts.PMEntitySupport" %>
<% try{ %><div class="cell ${pmfn:highlight(entity,field, item, field_value)}"><jsp:include page="<%= "../converters/"+field.visualize(es.prepareForConversion(field, item, field_value), operation, entity)+"&f="+field.getId() %>" flush="true" /></div><%}catch(Exception e){es.getPM().error(e);%><img width="16px" src='${es.context_path}/templates/${pm.template}/images/m_error.png' alt='error' /><%}%>